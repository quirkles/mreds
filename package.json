{
  "name": "mr-stats",
  "version": "1.9.1",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "node ./dist/index.js",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/index.ts --ignore client",
    "test:unit": "jest --runInBand",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npx eslint 'src/**' --fix",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "clean": "rimraf dist/",
    "build": "tsc",
    "encrypt-secrets": "gcloud kms encrypt --key=secrets-enc-key --keyring=madrid-reds --location=global --plaintext-file=src/constants/constants.ts --ciphertext-file=src/constants/constants.ts.enc",
    "decrypt-secrets": "gcloud kms decrypt --key=secrets-enc-key --keyring=madrid-reds --location=global --plaintext-file=src/constants/constants.ts --ciphertext-file=src/constants/constants.ts.enc",
    "copy-files": "copyfiles -u 1 src/**/*.graphql dist/"
  },
  "engines": {
    "node": "16.17.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-formidable": "^1.2.0",
    "@types/jest": "^29.1.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.8.4",
    "@types/nodemailer": "^6.4.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^4.3.8",
    "jest": "^29.1.1",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@google-cloud/logging": "^11.2.0",
    "@playwright/test": "^1.48.0",
    "@typegoose/typegoose": "^10.3.2",
    "apollo-server": "^3.12.0",
    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "bcrypt": "^5.0.0",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.32.0",
    "cookie-parser": "^1.4.4",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-formidable": "^1.2.0",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.2",
    "nodemailer": "^6.4.6",
    "pino": "^9.6.0",
    "playwright": "^1.48.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^2.0.0-beta.2",
    "uuid": "^11.1.0"
  }
}
